{
  "name": "police-fraud-detection-frontend",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    { 
      "src": "/(.*)", 
      "dest": "/" 
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://fraud-detection-api-2025.herokuapp.com",
    "REACT_APP_ENVIRONMENT": "production"
  }
}
